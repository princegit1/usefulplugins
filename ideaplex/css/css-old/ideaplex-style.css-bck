/************************ Tooltip css **********************/
.tooltip-popup{font-family:Oxygen; position:absolute; width:245px; bottom:0px; right:70%; background:rgba(255, 255, 255, 0.9); padding:10px; border-radius:10px; box-shadow:1px 1px 3px 0 rgba(0, 0, 0 , 0.5) ; opacity:0; z-index:-1;
-webkit-transition:opacity 0.1s ease-in-out, right 0.1s ease-in-out;
	-moz-transition:opacity 0.1s ease-in-out, right 0.1s ease-in-out;
	-o-transition:opacity 0.1s ease-in-out, right 0.1s ease-in-out;
	-ms-transition:opacity 0.1s ease-in-out, right 0.1s ease-in-out;
	transition: opacity 0.1s ease-in-out, right 0.1s ease-in-out;

}
.tooltip-popup.tooltip-active{ right:100%; opacity:1; z-index:10;  -webkit-transition:opacity 0.3s ease-in-out;
	-webkit-transition:opacity 0.4s ease-in-out, right 0.4s ease-in-out;
	-moz-transition:opacity 0.4s ease-in-out, right 0.4s ease-in-out;
	-o-transition:opacity 0.4s ease-in-out, right 0.4s ease-in-out;
	-ms-transition:opacity 0.4s ease-in-out, right 0.4s ease-in-out;
	transition: opacity 0.4s ease-in-out, right 0.4s ease-in-out;
	
	}
.tooltip-popup .tooltip-container{ position:relative;}
.tooltip-popup .close-popup{ color:#999999; position:absolute; right:0px; margin-right:5px; cursor:pointer;}
.tooltip-popup h4{ font-size:16px; color:#111; text-transform:uppercase;} 
.tooltip-popup h6{ font-size:14px; color:#666666;}
.tooltip-popup p{ font-size:13px; color:#999999;}
.tooltip-popup .popnav { margin-top:5px;}
.tooltip-popup .popnav ul li{ list-style:none; width:auto; display:inline-block; font-size:11px; color:#999999; border-right:1px solid #ccc; line-height:18px; float:left; padding:0 5px;}
.tooltip-popup .popnav ul li span.popup-videoicon{ width:18px; height:18px; display:block; background-position:-259px -242px; float:left; margin-right:5px; }
.tooltip-popup .popnav ul li span.popup-watch{ width:18px; height:18px; display:block; background-position:-297px  -242px; float:left;}
.tooltip-popup .popnav ul li span.popup-share{ width:18px; height:18px; display:block; background-position:-336px -242px; float:left;}
.tooltip-popup .popnav ul li a{ font-size:11px; color:#999999; text-decoration:none;}

/********************************* Box Arrow right ******************************/

.tooltip-popup:after, .tooltip-popup:before { left: 100%; top: 50%; border: solid transparent; content: " "; height: 0; width: 0; position: absolute; pointer-events: none; } 
.tooltip-popup:after { border-color: rgba(136, 183, 213, 0);  border-width: 30px; margin-top: -30px; } 
.tooltip-popup:before { border-color: rgba(194, 225, 245, 0); border-left-color:rgba(255, 255, 255, 0.9); border-width: 27px; margin-top: -6px;  }


/********************************* Box Arrow right ******************************/
#ideate .tooltip-popup:after, #ideate .tooltip-popup:before { left: -20%; top: 50%; border: solid transparent; content: " "; height: 0; width: 0; position: absolute; pointer-events: none; } 
#ideate .tooltip-popup:after { border-color: rgba(136, 183, 213, 0); border-width: 30px; margin-top: -30px; } 
#ideate .tooltip-popup:before { border-color: rgba(194, 225, 245, 0); border-right-color: rgba(255, 255, 255, 0.9); border-width: 27px; margin-top: -6px; }

/******************************************************** tootip for right section ******************************************/
