-webkit-transition: left 0.5s ease-in-out,opacity 0.1s ease-in-out;
	-moz-transition:left 0.5s ease-in-out,opacity 0.1s ease-in-out;
	-o-transition:left 0.5s ease-in-out,opacity 0.1s ease-in-out;
